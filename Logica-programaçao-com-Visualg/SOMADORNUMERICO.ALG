Algoritmo "somadorNumerico"

Var
   cont , n, s : inteiro


Inicio
    cont <- 1
    s <- 0
    Enquanto (cont <= 5) faca
        Escreva ("Digite o ", cont, "o. valor : ")
        leia(n)
        s <- s + n
        cont <- cont + 1
    FimEnquanto
    escreval("A soma de todos os valores foi ", s)
// Seção de Comandos, procedimento, funções, operadores, etc...


Fimalgoritmo