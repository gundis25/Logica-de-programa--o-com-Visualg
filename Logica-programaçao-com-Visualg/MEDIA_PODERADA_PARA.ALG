Algoritmo "media_poderada_para"

Var
  N, i : inteiro
  v1, v2, v3, mediap : real


Inicio
    escreva("Quantos casos voce vai digitar? ")
    leia(N)
    para i de 1 ate N faca
      escreval("Digite tres numeros:")
      leia(v1)
      leia(V2)
      leia(v3)
      mediap <- (v1 * 2 + v2 * 3 + v3 * 5) / 10
