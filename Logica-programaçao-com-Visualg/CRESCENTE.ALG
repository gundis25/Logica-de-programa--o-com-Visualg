Algoritmo "crescente"

Var
  x, y :inteiro


Inicio
      escreval("Digite um nomuro: ")
      leia(x)
      leia(y)
      enquanto x <> y faca
         se x > y entao
            escreval("Decrecente")
         senao
            escreval("CRESCENTE")
         fimse
         escreval("Digite outros dois numero: ")
         leia(x)
         leia(y)
      fimenquanto


Fimalgoritmo